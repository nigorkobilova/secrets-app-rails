<!DOCTYPE html>
<html>
<head>
  <title>SecretsApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<nav>
  <% if current_user %>
      <%= link_to "Log Out", session_path, method: :delete %>
  <% end %>
</nav>
<%= yield %>

</body>
</html>
